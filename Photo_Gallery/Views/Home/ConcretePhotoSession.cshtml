
@{
    Layout = null;
}

@model Photo_Gallery.Models.PhotoSession



<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>KateGalery</title>

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Free HTML5 Template by FREEHTML5.CO" />
    <meta name="keywords" content="free html5, free template, free bootstrap, html5, css3, mobile first, responsive" />
    <meta name="author" content="FREEHTML5.CO" />

    <meta property="og:title" content="" />
    <meta property="og:image" content="" />
    <meta property="og:url" content="" />
    <meta property="og:site_name" content="" />
    <meta property="og:description" content="" />
    <meta name="twitter:title" content="" />
    <meta name="twitter:image" content="" />
    <meta name="twitter:url" content="" />
    <meta name="twitter:card" content="" />
    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <link rel="shortcut icon" href="favicon.ico">
    <!-- Google Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Roboto+Slab:400,700|Roboto:400,300,700' rel='stylesheet' type='text/css'>
    <!-- Animate -->
    <link rel="stylesheet" href="~/Content/animate.css">
    <!-- Flexslider -->
    <link rel="stylesheet" href="~/Content/flexslider.css">
    <!-- Icomoon -->
    <link rel="stylesheet" href="~/Content/icomoon.css">
    <!-- Bootstrap  -->
    <link rel="stylesheet" href="~/Content/bootstrap.css">
    <link rel="stylesheet" href="~/Content/style.css">


    <!-- Modernizr JS -->
    <script src="~/Scripts/modernizr-2.6.2.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/gsap/1.18.0/TweenMax.min.js"></script>
    <!-- FOR IE9 below -->
    <!--[if lt IE 9]>
    <script src="js/respond.min.js"></script>
    <![endif]-->
</head>
<body>


    <div class="main">



        <div id="fh5co-header">
            <div class="container">
                <!-- Mobile Toggle Menu Button -->
                <a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle"><i></i></a>
                <div id="fh5co-logo">
                    <p class="transition">@Html.ActionLink("fotografy", "Index", "Home") <span id="spanpoin">.</span></p>
                </div>
                <div class="nav-menu">
                    <nav id="fh5co-main-nav">
                        <ul>
                            <li><a href="@Url.Action("Index", "Home")" data-nav-section="home">На главную</a></li>
                          
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
        <div class="row">

            <div class="container">
                <div class="row r-pb">
                    <div class="col-md-8 col-md-offset-2 text-center section-heading">
                        <div class="imagesDiv">
                            @{

                                List<string> photos = new List<string>(@Directory.GetFileSystemEntries(Model.PhotoReposURL.ToString()));
                                foreach (string photo in photos)
                                {
                                    string newStr = photo.Replace(@"\", @"/");
                                    newStr = newStr.Remove(0, newStr.LastIndexOf('/'));

                                    <div>
                                        <img src="@Url.Content("~/PhotoSessions/" + Model.PhotoSessionName + "/" + newStr)" class="img-responsive">
                                    </div>

                                }
                            }
                        </div>
                    </div>
                </div>

            </div>
        </div>
        <div id="fh5co-testimony" style="background-image: url(~/images/slide_3.jpg);" data-stellar-background-ratio="0.5">
          
            <div class="fh5co-overlay">
                <div class="photosession_name_span">
                    <span>@Model.PhotoSessionName</span>
                </div>
            </div>

            <div class="container">
                <div class="row">

                    <div class="description">
                        <h4 style=" color: white">@Model.PhotoSessionDescription</h4>

                    </div>


                </div>
            </div>
        </div>
        <div id="fh5co-footer">
            <div class="container">
                <div class="row">
                    <div class="col-md-6 col-sm-6 animate-box">

                    </div>
                    <div class="col-md-6 col-sm-6 animate-box">
                        <div class="fh5co-footer-widget">
                            <ul class="fh5co-social pull-right">

                                <li>
                                    <a href="#"><i class="icon-facebook"></i></a>
                                </li>

                                <li>
                                    <a href="#"><i class="icon-google"></i></a>
                                </li>
                                <li>
                                    <a href="#"><i class="icon-instagram"></i></a>
                                </li>

                            </ul>
                            <div class="fh5co-footer-widget">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </div>
        <!-- jQuery -->
        <script src="~/Scripts/jquery.min.js"></script>
        <!-- jQuery Easing -->
        <script src="~/Scripts/jquery.easing.1.3.js"></script>
        <!-- Bootstrap -->
        <script src="~/Scripts/bootstrap.min.js"></script>
        <!-- Waypoints -->
        <script src="~/Scripts/jquery.waypoints.min.js"></script>
        <!-- Stellar Parallax -->
        <script src="~/Scripts/jquery.stellar.min.js"></script>
        <!-- Flexslider -->
        <script src="~/Scripts/jquery.flexslider-min.js"></script>

        <script src="~/Scripts/ConcretePhotosession.js"></script>



        <script>



        </script>
</body>
</html>
